private void setGridStroke() {
  strokeWeight(0.25);
  noFill();
  stroke(0, 0, 200, 75);
}
private void setHighlightedGridStroke() {
  strokeWeight(0.75);
  noFill();
  stroke(0, 0, 200, 75);
}

private void setPatternStroke() {
  strokeWeight(1);
  noFill();
  stroke(50);
}

private void setLandmarkStroke() {
  strokeWeight(2);
  fill(50);
  stroke(50);
}
